<?xml version="1.0" encoding="utf-8"?>
<!--
    Disable ALL backup and data extraction.
    Privacy Shield contains no persistent user data to back up,
    and we explicitly prohibit cloud backup as a privacy measure.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="root" path="." />
    </cloud-backup>
    <device-transfer>
        <exclude domain="root" path="." />
    </device-transfer>
</data-extraction-rules>
